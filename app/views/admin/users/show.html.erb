<h1><%= 'Usuario actualizado:' %></h1>
<%= form_for(@user) do |f| %>
	<%= render :partial => 'form', :locals => { :f => f, :boton => 'Show'  } %>
<% end %>
<p>
	<%= link_to 'Editar', edit_admin_user_path(@user) %>
	<%= link_to 'Nuevo usuario', new_admin_user_path %>
	<%= link_to 'Listar', admin_users_path %>
</p>
